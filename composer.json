{
    "name": "aaroncadrian/collective-form",
    "description": "Laravel package",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.4",
        "illuminate/validation": ">=5.4",
        "laravelcollective/html": "^5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7",
        "orchestra/testbench": ">=3.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Adrian",
            "email": "aaroncadrian@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/Services/helpers.php"
        ],
        "psr-4": {
            "AaronAdrian\\CollectiveForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronAdrian\\CollectiveForm\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronAdrian\\CollectiveForm\\CollectiveFormServiceProvider"
            ]
        }
    }
}
